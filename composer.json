{
  "name": "contao-dd/advanced-classes-bundle",
  "type":"contao-bundle",
  "description": "Contao 4 advanced classes bundle",
  "license": "LGPL-3.0-or-later",
  "authors":[
    {
      "name":"Contao Stammtisch Dresden",
      "homepage":"http://www.gondao.de",
      "role":"Developer",
      "email": "kontakt@gondao.de"
    }
  ],
  "support" : {
    "email" : "develop@pdir.de",
    "issues" : "https://github.com/contao-dd/advanced-classes-bundle/issues",
    "source" : "https://github.com/contao-dd/advanced-classes-bundle"
  },
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "contao/easy-coding-standard": "^3.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoDD\\AdvancedClassesBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "ContaoDD\\AdvancedClassesBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
